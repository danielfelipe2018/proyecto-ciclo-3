@page
@model MedicalApp.App.FrontEnd.Pages.Sedes.CreateModel
@{
}
<h1>Nueva Sede</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="sede.NombreSede"></label>
        <input asp-for="sede.NombreSede" class="form-control">
        <span asp-validation-for="sede.NombreSede" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="sede.Direccion"></label>
        <input asp-for="sede.Direccion" class="form-control">
        <span asp-validation-for="sede.Direccion" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Crear sede</button>
</form>
<a asp-page="./Index" class="btn btn-primary">Regresar</a>