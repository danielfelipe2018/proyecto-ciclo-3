@page
@model MedicalApp.App.FrontEnd.Page.Pacientes.DetailsModel
@{
}
<h1>Detalle del paciente</h1> 
<table class="table table-dark">
    <tr>
        <th scope="col">Tipo Identificacion</th>
        <td>@Model.paciente.TipoIdentificacion</td>
    </tr>
    <tr>
        <th scope="col">Identificacion</th>
        <td>@Model.paciente.Identificacion</td>
    </tr>
    <tr>
        <th scope="col">Nombre</th>
        <td>@Model.paciente.Nombre</td>
    </tr>
    <tr>
        <th scope="col">Apellidos</th>
        <td>@Model.paciente.Apellido</td>
    </tr>
    <tr>
        <th scope="col">Genero</th>
        <td>@Model.paciente.Genero</td>
    </tr>
    <tr>
        <th scope="col">Telefono</th>
        <td>@Model.paciente.Telefono</td>
    </tr>
    <tr>
        <th scope="col">Correo</th>
        <td>@Model.paciente.Correo</td>
    </tr>
</table>
<a asp-page="./Index" class="btn btn-primary" >Regresar</a>
